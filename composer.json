{
    "name": "mermshaus/horne",
    "description": "Static output generator",
    "license": "MIT",
    "require": {
        "php": "^8.5",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-zlib": "*",
        "kaloa/filesystem": "0.1.0",
        "kaloa/renderer": "0.3.0",
        "rkr/recursive-array-accessor": "~2.0",
        "symfony/console": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^4",
        "phpstan/phpstan": "^2.1"
    },
    "bin": [
        "horne"
    ],
    "autoload": {
        "psr-4": {
            "Horne\\": "src/"
        }
    }
}
